<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="MainNavigateTabBar">
        <attr name="navigateTabTextColor" format="color|reference"/>
        <attr name="navigateTabSelectedTextColor" format="color|reference"/>
        <attr name="containerId" format="integer|reference"/>
        <attr name="navigateTabTextSize" format="dimension|reference"/>
    </declare-styleable>

    <declare-styleable name="CircleImageView">
        <attr name="border_width" format="dimension" />
        <attr name="border_color" format="color" />
    </declare-styleable>

    <declare-styleable name="ExpandPopTabView">
        <attr name="tab_toggle_btn_bg" format="reference"></attr>
        <attr name="tab_toggle_btn_color" format="color"></attr>
        <attr name="tab_toggle_btn_font_color" format="color"></attr>
        <attr name="tab_toggle_btn_font_size" format="dimension"></attr>
        <attr name="tab_pop_bg_color" format="color"></attr>
    </declare-styleable>

    <!-- 水电工个人主页 管理页面中的item-->
    <declare-styleable name="personManageItemView">
        <attr name="icon" format="reference"/>
        <attr name="text1" format="string"/>
        <attr name="text2" format="string"/>
        <attr name="text3" format="string"/>
        <attr name="text4" format="string"/>
        <attr name="text5" format="string"/>
    </declare-styleable>

    <declare-styleable name="PublishTextRowView">
        <attr name="PublishTextRowViewText" format="string"/>
        <attr name="PublishTextRowViewHiht" format="string"/>
        <attr name="PublishTextRowViewTextSize" format="dimension"/>
        <attr name="PublishTextRowViewTextColor" format="color"/>
        <attr name="PublishTextRowViewIsBottomLineShow" format="boolean"/>
        <attr name="PublishTextRowViewIsSelect" format="boolean"/>
        <attr name="PublishTextRowViewLeftWidth" format="dimension"/>
        <attr name="PublishTextRowViewGravity" format="integer"/>
    </declare-styleable>
</resources>